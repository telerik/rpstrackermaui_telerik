<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RPS.UI.Views.RPSFlyoutPage"
             xmlns:views="clr-namespace:RPS.UI.Views"
             Title="RPSFlyoutPage">
    
    <FlyoutPage.Flyout>
        <views:FlyoutMenuPage Title="Menu" x:Name="flyoutPage" IconImageSource="dotnet_bot.png"/>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:WelcomePage Title="Welcome" />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>